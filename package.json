{
  "name": "proxy-memoize",
  "description": "Intuitive magical memoization library with Proxy and WeakMap",
  "version": "1.2.0",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/proxy-memoize.git"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.modern.js",
  "react-native": "./dist/index.modern.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "module": "./dist/index.modern.js",
      "import": "./dist/index.modern.mjs",
      "default": "./dist/index.umd.js"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "microbundle build -f modern,umd",
    "postcompile": "cp dist/index.modern.mjs dist/index.modern.js && cp dist/index.modern.mjs.map dist/index.modern.js.map",
    "test": "run-s eslint tsc-test jest",
    "eslint": "eslint --ext .js,.ts --ignore-pattern dist .",
    "jest": "jest",
    "tsc-test": "tsc --project . --noEmit",
    "apidoc": "documentation readme src --section API --markdown-toc false --parse-extension ts"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts"
  },
  "keywords": [
    "proxy",
    "memoize",
    "selector"
  ],
  "license": "MIT",
  "dependencies": {
    "proxy-compare": "2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "documentation": "^14.0.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "immer": "^9.0.17",
    "jest": "^29.3.1",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4"
  }
}
